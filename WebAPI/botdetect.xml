<?xml version="1.0" encoding="utf-8" ?>
<botdetect xmlns="https://captcha.com/schema/net"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://captcha.com/schema/net 
     https://captcha.com/schema/net/botdetect-4.4.2.xsd">

  <captchaStyles>
    <captchaStyle>
      <name>jqueryFormCaptcha</name>
      <userInputID>userCaptchaInput</userInputID>
      <codeLength>4-6</codeLength>
    </captchaStyle>
  </captchaStyles>
  <captchaEndpoint>
    <accessControlAllowOriginHeaderEnabled>true</accessControlAllowOriginHeaderEnabled>
    <accessControlAllowOriginHeaderValue>http://localhost:61102</accessControlAllowOriginHeaderValue>
  </captchaEndpoint>
</botdetect>

